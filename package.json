{
  "name": "@natlibfi/passport-natlibfi-keycloak",
  "description": "Passport authentication strategy for National Library of Finland using Keycloak or local files",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [],
  "homepage": "https://github.com/natlibfi/passport-natlibfi-keycloak-js",
  "bugs": {
    "url": "https://github.com/natlibfi/passport-natlibfi-keycloak-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:natlibfi/passport-natlibfi-keycloak-js.git"
  },
  "license": "MIT",
  "version": "2.0.1-alpha.5",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "lint:dev": "eslint --fix src",
    "test:base": "cross-env NODE_ENV=test nyc mocha --require @babel/register",
    "test": "npm run lint && npm run test:base -- src/**/*.spec.js",
    "build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist",
    "coverage": "nyc --reporter=text npm run test:base -- src/**/*.spec.js",
    "watch": "cross-env NODE_ENV=test chokidar src -c 'npm test'"
  },
  "dependencies": {
    "@natlibfi/melinda-backend-commons": "^2.2.1",
    "@natlibfi/melinda-commons": "^13.0.6",
    "@natlibfi/passport-keycloak": "^2.0.3-alpha.3",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/register": "^7.22.15",
    "@natlibfi/eslint-config-melinda-backend": "^3.0.1",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.8",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  },
  "eslintConfig": {
    "extends": "@natlibfi/melinda-backend"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "maintained node versions"
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80
  }
}
