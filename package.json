{
  "name": "@natlibfi/passport-natlibfi-keycloak",
  "description": "Passport authentication strategy for National Library of Finland using Keycloak or local files",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [],
  "homepage": "https://github.com/natlibfi/passport-natlibfi-keycloak-js",
  "bugs": {
    "url": "https://github.com/natlibfi/passport-natlibfi-keycloak-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:natlibfi/passport-natlibfi-keycloak-js.git"
  },
  "license": "MIT",
  "version": "3.0.1",
  "main": "./dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "esbuild \"src/**/*.js\"  --sourcemap --platform=node --outdir=dist",
    "build:analyze": "npm run build -- --analyze",
    "lint": "eslint ./src",
    "lint:dev": "eslint --fix ./src",
    "test:base": "node --test",
    "test:report": "npm run test:base -- --experimental-test-coverage --test-reporter=spec",
    "test": "npm run lint && npm run test:base",
    "watch:test": "cross-env NODE_ENV=test node --watch --test",
    "dev": "npm run watch:test",
    "dev:debug": "cross-env LOG_LEVEL=debug DEBUG=@natlibfi/* NODE_ENV=test npm run watch:test"
  },
  "dependencies": {
    "@natlibfi/passport-keycloak": "^3.0.1",
    "debug": "^4.4.1",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0"
  }
}
